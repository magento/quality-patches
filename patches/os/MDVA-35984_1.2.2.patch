+    {
+        $this->getSourceItemIds = $getSourceItemIds;
+        $this->sourceItemIndexer = $sourceItemIndexer;
+    }
+
+    /**
+     * @param DecrementSourceItemQty $subject
+     * @param void $result
+     * @param SourceItemInterface[] $sourceItems
+     * @return void
